{
  "name": "@google/gemini-cli-accelos",
  "version": "0.1.0",
  "description": "Accelos AI Agent using Mastra framework",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:bundle": "ncc build src/production.ts -o build --minify",
    "build:binary": "npm run build && pkg dist/production.js --targets node20-linux-x64,node20-macos-x64,node20-win-x64 --out-path binaries",
    "build:all": "npm run build && npm run build:bundle && npm run build:binary",
    "start": "node dist/production.js",
    "start:dev": "tsx src/production.ts",
    "dev": "mastra dev",
    "dev:server": "tsx src/server.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "bin": {
    "accelos-server": "dist/production.js"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.0",
    "@ai-sdk/google": "^2.0.5",
    "@ai-sdk/openai": "^1.2.0",
    "@google/genai": "1.9.0",
    "@mastra/core": "^0.13.2",
    "@mastra/libsql": "^0.13.2",
    "@mastra/mcp": "latest",
    "@mastra/memory": "^0.12.2",
    "ai": "^4.0.53",
    "dotenv": "^17.1.0",
    "mastra": "^0.10.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vercel/ncc": "^0.38.1",
    "pkg": "^5.8.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "node_modules/@mastra/**/*.js"
    ],
    "assets": [
      "node_modules/@mastra/**/*",
      ".env"
    ],
    "outputPath": "binaries"
  },
  "keywords": [
    "mastra",
    "ai",
    "agent",
    "server"
  ],
  "author": "",
  "license": "ISC"
}
