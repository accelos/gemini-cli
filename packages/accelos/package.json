    "@mastra/mcp": "latest",
    "ai": "^4.0.53",
    "dotenv": "^17.1.0",
    "mastra": "^0.10.21",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
