{
  "name": "@google/gemini-cli-accelos",
  "version": "0.1.0",
  "description": "Accelos AI Agent using Mastra framework",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "node ../../scripts/build_package.js",
    "start": "node dist/index.js",
    "debug": "node --inspect-brk dist/index.js",
    "dev": "mastra dev",
    "dev:server": "tsx src/server.ts",
